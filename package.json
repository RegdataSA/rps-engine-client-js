{
  "name": "rps-engine-client-js",
  "version": "4.0.0",

  "description": "RPS Engine client for JavaScript/TypeScript",
  "keywords": ["REGDATA", "RPS Engine", "transformation", "encryption", "decryption"],
  "license": "Apache-2.0",
  "repository": "RegdataSA/rps-engine-client-js",
  "author": "RegdataSA Mykhailo Kryvoruchko",
  "type": "module",

  "files": [
    "lib",
    "dist",
    "package.json"
  ],

  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./lib": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",

  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },

  "lint-staged": {
    "*": "eslint --fix"
  },

  "peerDependencies": {
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "axios": "^0.28.1",
    "jsonschema": "^1.4.1"
  },
  "devDependencies": {
    "@gitart/eslint-config-ts": "^1.0.0",
    "@vitest/coverage-v8": "^1.5.0",
    "bumpp": "^9.4.0",
    "eslint": "8.57.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "standard-version": "^9.5.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },

  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  }
}
